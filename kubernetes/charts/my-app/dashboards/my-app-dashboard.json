{
  "annotations": {
    "list": []
  },
  "editable": true,
  "id": null,
  "links": [],
  "title": "My App Dashboard",
  "uid": "my-app-dashboard",
  "version": 1,
  "panels": [
    {
      "type": "gauge",
      "title": "Active Users",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "active_users",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request Latency",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_sum[5m]) / rate(http_request_duration_seconds_count[5m]))",
          "legendFormat": "Average Latency",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request Rate",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "{{method}} {{endpoint}} - {{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Prediction Feedback",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(prediction_feedback_total[5m])",
          "legendFormat": "{{original_prediction}} - {{user_feedback}}",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "id": 5,
      "targets": [
        {
          "expr": "sum(rate(prediction_feedback_total{version=\"v1\"}[5m]))",
          "legendFormat": "v1 - Overall Feedback Rate",
          "refId": "A"
        },
        {
          "expr": "sum(rate(prediction_feedback_total{version=\"v2\"}[5m]))",
          "legendFormat": "v2 - Overall Feedback Rate",
          "refId": "B"
        }
      ],
      "title": "Overall Feedback Rate by Version",
      "type": "timeseries"
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  }
}